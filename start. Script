#!/bin/bash

AWS_REGION="ap-south-1"
ACCOUNT_ID="089358501446"
IMAGE="$ACCOUNT_ID.dkr.ecr.$AWS_REGION.amazonaws.com/test-codepipeline:latest"

echo "Starting container from image: $IMAGE"

# Run fresh container
docker run -d -p 80:80 --name test-codepipeline $IMAGE

echo "Container started successfully."
